{
  "manifest_version": 3,
  "name": "ArkhamDB-display-set-name",
  "version": "1.0",
  "description": "Replaces scenario pack names with corresponding set names",
  "action": {
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://arkhamdb.com/*",
        "https://pl.arkhamdb.com/*"
      ],
      "js": [
        "ArkhamDB-setname-replace.js"
      ]
    }
  ]
}
